class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.length() != t.length()) return false;
        
        vector<int> sHash(128, 0);
        vector<int> tHash(128, 0);

        for (int i = 0; i < s.length(); i++) {
            sHash[s[i]]++;
            tHash[t[i]]++;
        }

        for(int i=0;i<s.length();i++){
            if(sHash[t[i]] != tHash[t[i]]) return false;
        }

        return true;

    }
};
